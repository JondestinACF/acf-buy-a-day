    "bcryptjs": "^2.4.3",
    "csv-stringify": "^6.4.6",
    "date-fns": "^3.3.1",
    "nanoid": "^5.0.6",
    "next": "14.1.3",
    "next-auth": "^4.24.7",
    "nodemailer": "^6.9.12",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "resend": "^6.9.2",
